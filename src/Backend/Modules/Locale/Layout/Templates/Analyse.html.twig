          {{ macro.buttonIcon(geturl('ExportAnalyse' ) ~ '&language=' ~ language, 'plus', 'lbl.Export'|trans|capitalize ) }}
        </div>
      </div>
    {% endif %}
{% endblock %}

{% block content %}
  <div class="row fork-module-content">
