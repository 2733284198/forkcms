{% if not cookieBarHide %}
  <div id="cookieBar" class="alert alert-default">
    <div class="container">
      {{ 'msg.CookiesWarning'|trans|raw }}
      <a href="#" id="cookieBarAgree" class="btn btn-xs btn-success">{{ 'lbl.IAgree'|trans|ucfirst }}</a>
      <a href="#" id="cookieBarDisagree" class="btn btn-xs btn-danger">{{ 'lbl.IDisagree'|trans|ucfirst }}</a>
    </div>
  </div>
{% endif %}
